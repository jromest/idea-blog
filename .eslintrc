{
  "extends": ["plugin:jsx-a11y/recommended"],
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": ["react", "jsx-a11y"],
  "globals": {
    "graphql": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  }
}
